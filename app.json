{
  "expo": {
    "name": "AloDieuDuong",
    "slug": "AloDieuDuong",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-document-picker", 
        {
          "documentPermissionMessage": "Allow $(Product_NAME) to access documents" 
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": {
            "ios": "Allow $(PRODUCT_NAME) to use your camera",
            "android": {
              "title": "Camera Permission",
              "message": "Allow $(PRODUCT_NAME) to access your camera",
              "buttonPositive": "Allow",
              "buttonNegative": "Deny"
            }
          },
          "photosPermission": {
            "ios": "Allow $(PRODUCT_NAME) to access your photos",
            "android": {
              "title": "Photos Permission",
              "message": "Allow $(PRODUCT_NAME) to access your photos",
              "buttonPositive": "Allow",
              "buttonNegative": "Deny"
            }
          }
        }
      ]
    ]
  }
}
